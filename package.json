{
  "name": "hantenabookmark-replace-tags",
  "version": "1.0.3",
  "description": "A CLI that replace all hatena bookmark tags.",
  "homepage": "https://github.com/azu/hantenabookmark-replace-tags",
  "bugs": {
    "url": "https://github.com/azu/hantenabookmark-replace-tags/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/azu/hantenabookmark-replace-tags.git"
  },
  "license": "MIT",
  "author": "azu",
  "files": [
    "bin/",
    "lib/",
    "src/"
  ],
  "main": "lib/hantenabookmark-replace-tags.js",
  "types": "lib/hantenabookmark-replace-tags.d.ts",
  "bin": {
    "hantenabookmark-replace-tags": "./bin/hantenabookmark-replace-tags.js"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc -p .",
    "clean": "rimraf lib/",
    "get-token": "electron ./electron/index.js",
    "hantenabookmark-replace-tags": "dotenv ./bin/hantenabookmark-replace-tags.js",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "prepublish": "npm run --if-present build",
    "test": "mocha \"test/**/*.ts\"",
    "watch": "tsc -p . --watch"
  },
  "husky": {
    "hooks": {
      "precommit": "lint-staged"
    }
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": false,
    "tabWidth": 4
  },
  "dependencies": {
    "cross-fetch": "^3.0.1",
    "dotenv": "^6.2.0",
    "electron-authentication-hatena": "^3.0.1",
    "hatebu-mydata-parser": "^1.0.0",
    "hatena-bookmark-api": "^1.0.1",
    "meow": "^5.0.0",
    "p-all": "^1.0.0"
  },
  "devDependencies": {
    "@types/meow": "^5.0.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.21",
    "@types/p-all": "^1.0.0",
    "@types/p-queue": "^3.0.0",
    "cross-env": "^5.2.0",
    "dotenv-cli": "^1.4.0",
    "electron": "^4.0.4",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.1",
    "mocha": "^5.2.0",
    "prettier": "^1.9.2",
    "rimraf": "^2.6.3",
    "ts-node": "^8.0.2",
    "ts-node-test-register": "^8.0.0",
    "typescript": "^3.3.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "prettier --write",
      "git add"
    ]
  }
}
